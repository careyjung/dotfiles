#!/usr/bin/env bash

cd ~

[ -d .ssh ] || tar xvf Documents/Keys/ssh.tar
[ -d .gnupg ] || tar xvf Documents/Keys/gnupg.tar

if [ ! -d .git ]; then
    git init
    git remote add origin git@github.com:careyjung/dotfiles.git
    git fetch
    git checkout origin/master --force --track
fi

echo Ok, now run ~/bin/setup

exit 0
